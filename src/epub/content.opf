		<dc:title id="title">The Conjure Woman</dc:title>
		<meta property="file-as" refines="#title">Conjure Woman, The</meta>
		<dc:subject id="subject-1">African Americans -- Fiction</dc:subject>
		<dc:subject id="subject-2">Southern States -- Social life and customs -- Fiction</dc:subject>
		<meta property="authority" refines="#subject-1">LCSH</meta>
		<meta property="term" refines="#subject-1">sh2007100196</meta>
		<meta property="authority" refines="#subject-2">LCSH</meta>
